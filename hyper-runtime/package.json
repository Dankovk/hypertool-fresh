{
  "name": "@hypertool/runtime",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./controls": {
      "types": "./dist/controls/index.d.ts",
      "import": "./dist/controls/index.js"
    },
    "./frame": {
      "types": "./dist/frame/index.d.ts",
      "import": "./dist/frame/index.js"
    }
  },
  "scripts": {
    "build": "bun run build:bundles && bun run build:types",
    "build:bundles": "bun run build.config.ts",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly",
    "dev": "bun run build-dev.config.ts"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "p5": "^1.9.0",
    "three": "^0.160.0",
    "tweakpane": "^4.0.5"
  },
  "devDependencies": {
    "@types/p5": "^1.6.3",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.160.0",
    "@types/bun": "^1.3.0",
    "p5": "^1.9.0",
    "three": "^0.160.0",
    "tweakpane": "^4.0.5",
    "typescript": "^5.6.2"
  }
}
