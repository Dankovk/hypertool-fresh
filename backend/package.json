{
  "name": "@hypertools/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "NODE_ENV=production bun run src/index.js",
    "build": "rm -rf public/deps && mkdir public/deps/ && bun run build:runtime && cp -r ../packages/runtime public/deps/",
    "build:runtime": "cd ../packages/runtime && bun run build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.35",
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/openai": "^2.0.42",
    "@hono/node-server": "^1.14.0",
    "@hypertool/shared-config": "workspace:*",
    "@hypertool/runtime": "workspace:*",
    "ai": "^5.0.60",
    "diff": "^8.0.2",
    "hono": "^4.6.14",
    "immer": "^10.1.3",
    "jszip": "3.10.1",
    "llm-diff-patcher": "^0.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.14.12",
    "bun-types": "^1.1.38",
    "typescript": "5.6.2"
  }
}
